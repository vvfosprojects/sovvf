{
  "name": "sala-operativa-so115",
  "version": "0.3.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "post-build": "node ./build/post-build.js",
    "devel-fake-cas": "cd fake-cas && npm run start-dev",
    "devel-start": "ng serve --host 0.0.0.0 --disable-host-check",
    "devel-start-no-livereload": "ng serve --host 0.0.0.0 --disable-host-check --live-reload false",
    "build-prod": "ng build --configuration=production --base-href='./' && npm run post-build",
    "build-prod-test": "ng build --configuration=production-test --base-href='./' && npm run post-build",
    "build-prod-demo": "ng build --configuration=production-demo --base-href='./' && npm run post-build",
    "build-prod-watch": "ng build --configuration=production-watch && npm run post-build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "avvia": "concurrently \"npm:devel-fake-cas\" \"npm:devel-start\"",
    "avvia-no-livereload": "concurrently \"npm:devel-fake-cas\" \"npm:devel-start-no-livereload\"",
    "lite-server": "lite-server --baseDir=\"./dist/SO115\"",
    "watch-serve": "ng build --watch --configuration=production-watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.13",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~11.2.13",
    "@angular/compiler": "~11.2.13",
    "@angular/core": "~11.2.13",
    "@angular/forms": "~11.2.13",
    "@angular/localize": "^11.2.13",
    "@angular/platform-browser": "~11.2.13",
    "@angular/platform-browser-dynamic": "~11.2.13",
    "@angular/router": "~11.2.13",
    "@arcgis/core": "^4.20.2",
    "@aspnet/signalr": "^1.1.4",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ng-select/ng-select": "^7.2.0",
    "@ngx-progressbar/core": "^5.3.2",
    "@ngx-progressbar/http": "^5.3.2",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@ngxs/form-plugin": "^3.7.2",
    "@ngxs/logger-plugin": "^3.7.2",
    "@ngxs/router-plugin": "^3.7.2",
    "@ngxs/storage-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "@swimlane/ngx-charts": "^17.0.1",
    "angular-popper": "^2.0.1",
    "bootstrap": "^4.6.0",
    "core-js": "^3.6.5",
    "hoek": "^5.0.4",
    "immer": "^9.0.5",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ng-sidebar": "^9.4.2",
    "ngx-chips": "^2.2.2",
    "ngx-clipboard": "^13.0.1",
    "ngx-filter-pipe": "^2.1.2",
    "ngx-timeago": "^1.0.4",
    "ngx-toastr": "^14.0.0",
    "ngx-treeview": "^10.0.2",
    "ngx-ui-loader": "^11.0.0",
    "ngx-ui-switch": "^11.0.1",
    "npm": "^7.21.1",
    "popper.js": "^1.15.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
