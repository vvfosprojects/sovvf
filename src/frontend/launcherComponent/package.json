    "@angular/compiler-cli": "~11.2.13",
    "@angular/language-service": "~11.2.13",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "lite-server": "^2.4.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
