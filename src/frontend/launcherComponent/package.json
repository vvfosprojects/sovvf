{
  "name": "sala-operativa-so115",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "post-build": "node ./build/post-build.js",
    "devel-fake-cas": "cd fake-cas && npm run start-dev",
    "devel-start": "ng serve --host 0.0.0.0 --disable-host-check",
    "devel-start-no-livereload": "ng serve --host 0.0.0.0 --disable-host-check --live-reload false",
    "build-prod": "ng build --configuration=production --base-href='./' && npm run post-build",
    "build-prod-test": "ng build --configuration=production-test --base-href='./' && npm run post-build",
    "build-prod-demo": "ng build --configuration=production-demo --base-href='./' && npm run post-build",
    "build-prod-watch": "ng build --configuration=production-watch && npm run post-build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "avvia": "concurrently \"npm:devel-fake-cas\" \"npm:devel-start\"",
    "avvia-no-livereload": "concurrently \"npm:devel-fake-cas\" \"npm:devel-start-no-livereload\"",
    "lite-server": "lite-server --baseDir=\"./dist/SO115\"",
    "watch-serve": "ng build --watch --configuration=production-watch"
  },
  "private": true,
